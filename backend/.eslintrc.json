{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
      "no-underscore-dangle": 0,
      "linebreak-style": 0,
      "no-mixed-operators": 0,
      "max-len": [2, { "code": 150 }]
    }
}
